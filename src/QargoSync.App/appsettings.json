{
  "QargoSettings": {
    "Master": {
      "BaseUrl": "${QARGO_MASTER_BASE_URL}",
      "ClientId": "${QARGO_MASTER_CLIENT_ID}",
      "ClientSecret": "${QARGO_MASTER_CLIENT_SECRET}"
    },
    "Target": {
      "BaseUrl": "${QARGO_TARGET_BASE_URL}",
      "ClientId": "${QARGO_TARGET_CLIENT_ID}",
      "ClientSecret": "${QARGO_TARGET_CLIENT_SECRET}"
    }
  },
  "SynchronizationSettings": {
    "StartDate": "2025-01-01T00:00:00Z",
    "EndDate": "2025-12-31T23:59:59Z",
    "DryRun": false,
    "BatchSize": 100
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "QargoSync": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/qargo-sync-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  }
}